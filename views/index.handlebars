<div class="row">
    <div class="col-sm-12">
        <h1 id="taqueria">Taqueria El Apache</h1>
    </div>
</div>
<div class="row">
    <div class="col-sm-6">
        <h2>Tacos Ordered!</h2>
        <ul>
            {{#each tacos}}
            {{#unless devoured}}
            {{> tacos/tacoBlock devoured=false}}
            {{/unless}}
            {{/each}}
        </ul>
    </div>
    <div class-"col-sm-6">
        <h2>Tacos Devoured!</h2>
        <ul>
            {{#each tacos}}
            {{#if devoured}}
            {{> tacos/tacoBlock devoured=true}}
            {{/if}}
            {{/each}}
        </ul>
    </div>
</div>

<h2>Order another Taco!</h2>
<form class="create-form">

    <div class="form-group">
        <label for="ca">Type of Taco:</label>
        <input type="text" id="ca" name="name">
    </div>

    <button type="submit">Place Order</button>
</form>